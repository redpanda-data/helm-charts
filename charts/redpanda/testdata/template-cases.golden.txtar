    PREFIX_TEMPLATE="$POD_ORDINAL-XYZ-$(echo -n $HOST_IP_ADDRESS | sha256sum | head -c 7)"
    ADVERTISED_KAFKA_ADDRESSES+=("{\"address\":\"rp0.REDPANDAREDPANDAREDPANDA-testing\",\"name\":\"default\",\"port\":31092}")

    PREFIX_TEMPLATE="$POD_ORDINAL-XYZ-$(echo -n $HOST_IP_ADDRESS | sha256sum | head -c 7)"
